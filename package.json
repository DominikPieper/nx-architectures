  },
  "devDependencies": {
    "@astrojs/ts-plugin": "^1.2.0",
    "@nx/devkit": "16.10.0",
    "@types/fs-extra": "^11.0.2",
    "all-contributors-cli": "^6.26.1",
    "fs-extra": "^11.1.1",
    "hast-util-from-html": "^1.0.2",
    "hast-util-to-string": "^2.0.0",
    "hastscript": "^7.2.0",
    "nx": "16.10.0",
